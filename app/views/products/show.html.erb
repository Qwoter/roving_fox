<div class="show_form_admin">
  <p id="notice"><%= notice %></p>

  <p>
    <strong>Name:</strong>
    <%= @product.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= @product.email %>
  </p>

  <p>
    <strong>Phone number:</strong>
    <%= @product.phone_number %>
  </p>

  <p>
    <strong>Product:</strong>
    <%= @product.product %>
